{
  "name": "integracao-loggi-bff",
  "version": "0.3.7",
  "description": "Integatrion with Loggi's delivery system API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "babel src -s -D -d dist --presets es2015,stage-0 && node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "./node_modules/mocha/bin/mocha \"./src/test/**/*.js\" --require babel-register --reporter spec || exit 0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Jason Miller <jason@developit.ca>",
  "currentDeveloper": "Am√≥s Batista <amos.silva@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^6.2.0",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.0.0",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^5.2.11",
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^3.1.1",
    "mocha": "^6.1.3",
    "nodemon": "^1.9.2",
    "sinon": "^7.3.1"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme"
}
