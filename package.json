{
  "name": "integracao-loggi-bff",
  "version": "0.3.7",
  "description": "Integatrion with Loggi's delivery system API.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets @babel/env\"",
    "build": "npm install && babel src -d dist --presets @babel/env",
    "start": "node dist/index.js",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Jason Miller <jason@developit.ca>",
  "currentDeveloper": "Am√≥s Batista <amos.silva@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.418.0",
    "@babel/cli": "^7.18.10",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^6.2.0",
    "express": "^4.13.3",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "morgan": "^1.8.0",
    "mysql": "^2.16.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.1.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^6.19.0",
    "superagent": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^3.1.1",
    "jest": "^29.7.0",
    "mocha": "^6.1.3",
    "nodemon": "^1.9.2",
    "sinon": "^7.3.1",
    "ts-jest": "^29.1.2"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme"
}
